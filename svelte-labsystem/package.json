{
  "name": "svelte-labsystem",
  "version": "0.0.1-alpha",
  "scripts": {
    "build": "rollup -c",
    "dev": "rm -r ./public/build/ && rollup -c -w",
    "start": "sirv public",
    "validate": "svelte-check",
    "test": "jest src",
    "test:u": "jest src -u",
    "test:watch": "npm run test -- --watch"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@rollup/plugin-typescript": "^6.0.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/svelte": "^3.0.0",
    "@tsconfig/svelte": "^1.0.0",
    "babel-jest": "^26.5.2",
    "jest": "^26.5.2",
    "node-sass": "^4.14.1",
    "prettier": "^2.1.2",
    "prettier-plugin-svelte": "^1.4.0",
    "rollup": "^2.3.4",
    "rollup-plugin-gzip": "^2.5.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^6.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "sirv-cli": "^1.0.0",
    "svelte": "^3.0.0",
    "svelte-check": "^1.0.0",
    "svelte-htm": "^1.1.1",
    "svelte-jester": "^1.1.5",
    "svelte-preprocess": "^4.3.2",
    "tslib": "^2.0.0",
    "typescript": "^3.9.3"
  },
  "dependencies": {
    "bulma": "^0.9.1"
  },
  "svelte": "src/index.js",
  "types": "@types/index.d.ts",
  "jest": {
    "setupFiles": [
      "./throwOnPropWarnings"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.svelte$": "svelte-jester"
    },
    "moduleFileExtensions": [
      "js",
      "svelte"
    ]
  },
  "files": [
    "src/*.svelte",
    "src/index.js",
    "@types"
  ]
}
