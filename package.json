{
  "name": "labstorybook",
  "version": "0.0.1-alpha",
  "engines": {
    "node": ">=12.14.0",
    "npm": ">=6.13.4"
  },
  "scripts": {
    "start": "start-storybook -s ./static -p 8000 --ci --docs",
    "build": "build-storybook -o dist -s ./static --docs && gzipper --verbose ./dist",
    "deploy": "npm run build && gh-pages -d dist"
  },
  "dependencies": {
    "@babel/core": "^7.7.5",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@storybook/addon-docs": "^5.2.8",
    "@storybook/react": "^5.2.8",
    "babel-loader": "^8.0.6",
    "gh-pages": "^2.1.1",
    "node-sass": "^4.13.0",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "sass-loader": "^8.0.0"
  },
  "devDependencies": {
    "gzipper": "^3.4.2"
  }
}
